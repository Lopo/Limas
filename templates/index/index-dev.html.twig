	window.theme = 'classic';
{% for themeId, themeName in themes %}
	window.themes['{{ themeId }}'] = {
		themeUri: '/js/packages/extjs/build/classic/theme-{{ themeId }}/resources/theme-{{ themeId }}-all.css',
		themeUxUri: '/js/packages/extjs/build/packages/ux/classic/{{ themeId }}/resources/ux-all.css',
		themeName: '{{ themeName }}'
	};
{% endfor %}
	window.parameters = {{ parameters|json_encode|raw }};
</script>
