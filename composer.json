{
	"name": "lopo/limas",
	"description": "Open Source Inventory Software",
	"type": "project",
	"keywords": [
		"partkeepr",
		"inventory"
	],
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Pavol HluchÃ½",
			"email": "lopo@losys.eu",
			"role": "Project Lead, Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Lopo/Limas/issues",
		"source": "https://github.com/Lopo/Limas"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.1",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"api-platform/core": "^2.6",
		"atelierspierrot/famfamfam-silk-sprite": "^1.0",
		"doctrine/annotations": "^1.0",
		"doctrine/doctrine-bundle": "^2.6",
		"doctrine/doctrine-fixtures-bundle": "^3.4",
		"doctrine/doctrine-migrations-bundle": "^3.2",
		"doctrine/orm": "^2.11",
		"gesdinet/jwt-refresh-token-bundle": "^1.1",
		"gitonomy/gitlib": "^1.3",
		"guzzlehttp/guzzle": "^7.4",
		"icons/fugue": "^3.5",
		"knplabs/knp-gaufrette-bundle": "^0.7.2",
		"liip/imagine-bundle": "^2.8",
		"nette/utils": "^3.2",
		"oomphinc/composer-installers-extender": "^2.0",
		"phpdocumentor/reflection-docblock": "^5.3",
		"phpoffice/phpspreadsheet": "^1.23",
		"phpstan/phpdoc-parser": "^1.4",
		"predis/predis": "^2.0",
		"ramsey/uuid": "^4.3",
		"sencha/extjs": "7.0.0",
		"sensio/framework-extra-bundle": "^6.1",
		"sonata-project/exporter": "^2.13",
		"stof/doctrine-extensions-bundle": "^1.7",
		"symfony/asset": "6.1.*",
		"symfony/console": "6.1.*",
		"symfony/doctrine-messenger": "6.1.*",
		"symfony/dotenv": "6.1.*",
		"symfony/expression-language": "6.1.*",
		"symfony/flex": "^2",
		"symfony/form": "6.1.*",
		"symfony/framework-bundle": "6.1.*",
		"symfony/http-client": "6.1.*",
		"symfony/intl": "6.1.*",
		"symfony/ldap": "6.1.*",
		"symfony/mailer": "6.1.*",
		"symfony/mime": "6.1.*",
		"symfony/monolog-bundle": "^3.0",
		"symfony/notifier": "6.1.*",
		"symfony/process": "6.1.*",
		"symfony/property-access": "6.1.*",
		"symfony/property-info": "6.1.*",
		"symfony/proxy-manager-bridge": "6.1.*",
		"symfony/runtime": "6.1.*",
		"symfony/security-bundle": "6.1.*",
		"symfony/serializer": "6.1.*",
		"symfony/string": "6.1.*",
		"symfony/translation": "6.1.*",
		"symfony/twig-bundle": "6.1.*",
		"symfony/validator": "6.1.*",
		"symfony/web-link": "6.1.*",
		"symfony/webapp-meta": "^1.0",
		"symfony/webpack-encore-bundle": "^1.12",
		"symfony/yaml": "6.1.*",
		"twig/extra-bundle": "^2.12|^3.0",
		"twig/twig": "^2.12|^3.0"
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"symfony/flex": true,
			"symfony/runtime": true,
			"phpstan/extension-installer": true,
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true
		},
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Limas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Limas\\Tests\\": "tests/"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		],
		"app:phpstan": "vendor/bin/phpstan analyse"
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.1.*"
		},
		"installer-types": [
			"jslib-zip",
			"asset-zip"
		],
		"installer-paths": {
			"public/js/packages/{$name}": [
				"type:jslib-zip"
			],
			"assets/zip/{$name}": [
				"type:asset-zip"
			]
		}
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"liip/functional-test-bundle": "4.6.0-alpha1",
		"liip/test-fixtures-bundle": "^2.4",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan": "^1.5",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"phpstan/phpstan-doctrine": "^1.3",
		"phpstan/phpstan-strict-rules": "^1.1",
		"phpstan/phpstan-symfony": "^1.1",
		"spaze/phpstan-disallowed-calls": "^2.3",
		"symfony/debug-bundle": "6.1.*",
		"symfony/maker-bundle": "^1.0",
		"symfony/phpunit-bridge": "^6.1",
		"symfony/stopwatch": "6.1.*",
		"symfony/web-profiler-bundle": "6.1.*"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "sencha/extjs",
				"version": "7.0.0",
				"type": "jslib-zip",
				"dist": {
					"url": "https://cdn.sencha.com/ext/gpl/ext-7.0.0-gpl.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "icons/fugue",
				"version": "3.5.6",
				"type": "asset-zip",
				"dist": {
					"url": "https://p.yusukekamiyamane.com/icons/downloads/fugue-icons-3.5.6.zip",
					"type": "zip"
				}
			}
		}
	]
}
