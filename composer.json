{
	"name": "lopo/limas",
	"description": "Open Source Inventory Software",
	"type": "project",
	"keywords": [
		"partkeepr",
		"inventory"
	],
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Pavol HluchÃ½",
			"email": "lopo@losys.eu",
			"role": "Project Lead, Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Lopo/Limas/issues",
		"source": "https://github.com/Lopo/Limas"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.2",
		"ext-ctype": "*",
		"ext-dom": "*",
		"ext-iconv": "*",
		"api-platform/core": "^3.2",
		"atelierspierrot/famfamfam-silk-sprite": "^1.0",
		"doctrine/doctrine-bundle": "^2.11",
		"doctrine/doctrine-migrations-bundle": "^3.3",
		"doctrine/orm": "^2.17",
		"gesdinet/jwt-refresh-token-bundle": "^1.2",
		"gitonomy/gitlib": "^1.4",
		"guzzlehttp/guzzle": "^7.8",
		"icons/fugue": "^3.5",
		"knplabs/knp-gaufrette-bundle": "^0.9.0",
		"liip/imagine-bundle": "^2.12",
		"nette/utils": "^4.0",
		"oomphinc/composer-installers-extender": "^2.0",
		"phpdocumentor/reflection-docblock": "^5.3",
		"phpoffice/phpspreadsheet": "^1.29",
		"phpstan/phpdoc-parser": "^1.24",
		"predis/predis": "^2.2",
		"ramsey/uuid": "^4.7",
		"sencha/extjs": "7.0.0",
		"sonata-project/exporter": "^3.3",
		"stof/doctrine-extensions-bundle": "^1.10",
		"symfony/asset": "7.0.*",
		"symfony/asset-mapper": "7.0.*",
		"symfony/console": "7.0.*",
		"symfony/dotenv": "7.0.*",
		"symfony/expression-language": "7.0.*",
		"symfony/flex": "^2",
		"symfony/framework-bundle": "7.0.*",
		"symfony/intl": "7.0.*",
		"symfony/ldap": "7.0.*",
		"symfony/mime": "7.0.*",
		"symfony/monolog-bundle": "^3.0",
		"symfony/notifier": "7.0.*",
		"symfony/process": "7.0.*",
		"symfony/property-access": "7.0.*",
		"symfony/property-info": "7.0.*",
		"symfony/runtime": "7.0.*",
		"symfony/security-bundle": "7.0.*",
		"symfony/serializer": "7.0.*",
		"symfony/string": "7.0.*",
		"symfony/translation": "7.0.*",
		"symfony/twig-bundle": "7.0.*",
		"symfony/validator": "7.0.*",
		"symfony/web-link": "7.0.*",
		"symfony/webpack-encore-bundle": "^2.1",
		"symfony/yaml": "7.0.*",
		"twig/twig": "^2.12|^3.0"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true,
			"symfony/flex": true,
			"symfony/runtime": true,
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true,
			"phpstan/extension-installer": true
		},
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Limas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Limas\\Tests\\": "tests/"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*",
		"symfony/polyfill-php82": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"importmap:install": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		],
		"app:phpstan": "vendor/bin/phpstan analyse --memory-limit=512M"
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "7.0.*"
		},
		"installer-types": [
			"jslib-zip",
			"asset-zip"
		],
		"installer-paths": {
			"public/js/packages/{$name}": [
				"type:jslib-zip"
			],
			"assets/zip/{$name}": [
				"type:asset-zip"
			]
		}
	},
	"require-dev": {
		"doctrine/doctrine-fixtures-bundle": "^3.5",
		"liip/test-fixtures-bundle": "^2.7",
		"phpstan/extension-installer": "^1.3",
		"phpstan/phpstan": "^1.10",
		"phpstan/phpstan-deprecation-rules": "^1.1",
		"phpstan/phpstan-doctrine": "^1.3",
		"phpstan/phpstan-strict-rules": "^1.5",
		"phpstan/phpstan-symfony": "^1.3",
		"phpunit/phpunit": "^9.5",
		"roave/security-advisories": "dev-latest",
		"spaze/phpstan-disallowed-calls": "^2.16",
		"symfony/browser-kit": "7.0.*",
		"symfony/css-selector": "7.0.*",
		"symfony/debug-bundle": "7.0.*",
		"symfony/maker-bundle": "^1.0",
		"symfony/phpunit-bridge": "^7.0",
		"symfony/stopwatch": "7.0.*",
		"symfony/web-profiler-bundle": "7.0.*"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "sencha/extjs",
				"version": "7.0.0",
				"type": "jslib-zip",
				"dist": {
					"url": "https://cdn.sencha.com/ext/gpl/ext-7.0.0-gpl.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "icons/fugue",
				"version": "3.5.6",
				"type": "asset-zip",
				"dist": {
					"url": "https://p.yusukekamiyamane.com/icon/downloads/fugue-icons-3.5.6.zip",
					"type": "zip"
				}
			}
		}
	]
}
